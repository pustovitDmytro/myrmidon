{
  "name": "myrmidon",
  "version": "1.5.3",
  "description": "package myrmidon provides a list of common utilities for comfortable work with js projects.",
  "main": "./lib",
  "scripts": {
    "prepare": "babel src --out-dir ./lib --copy-files && husky install",
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "test:security": "npm audit && npm run test:lock",
    "test:lint": "eslint src bin tests tools",
    "test:mocha": "mocha  --config .mocharc.json 'tests/**/*test.js'",
    "test:package": "./bin/test-package.sh",
    "test:coverage": "BUILD=0 MODE=test nyc mocha --config .mocharc.json 'tests/**/*test.js'",
    "test:debt": "jscpd src --config .jscpd.json",
    "test:lock": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm",
    "test:build": "npm run build && BUILD=1 mocha --config .mocharc.json 'tests/**/*.test.js'",
    "save-examples": "SAVE_EXAMPLES=tmp/examples.json mocha  --config .mocharc.json 'tests/**/*.test.js' --grep '@example'",
    "coverage-check": "MODE=test nyc check-coverage",
    "test": "npm run test:lint && npm run test:security && BUILD=1 npm run test:mocha",
    "sort-deps": "npm remove --save example; npm remove --save-dev example",
    "semantic-release": "npx semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pustovitDmytro/myrmidon"
  },
  "author": "Dmytro Pustovit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pustovitDmytro/myrmidon/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://myrmidonjs.readthedocs.io",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/lint": "^12.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "cls-hooked": "^4.2.2",
    "conventional-changelog-eslint": "^3.0.1",
    "coveralls": "^3.0.3",
    "danger": "^10.6.4",
    "docopt": "^0.6.2",
    "documentation": "^13.0.0",
    "escodegen": "^2.0.0",
    "eslint": "^7.0.0",
    "eslint-config-incredible": "^2.1.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-censor": "^1.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-regexp": "^0.11.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.7.0",
    "eslint-plugin-unicorn": "^33.0.0",
    "esprima": "^4.0.1",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.3.1",
    "husky": "^6.0.0",
    "jscpd": "^3.3.25",
    "lockfile-lint": "^4.6.2",
    "mdinclude": "^0.7.1",
    "mocha": "^9.0.0",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.0.0",
    "remark": "^13.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "remark-toc": "^7.0.0",
    "rollup": "^2.45.2",
    "semantic-release": "^17.4.2",
    "semantic-release-telegram": "^1.2.0",
    "uuid": "^8.0.0"
  }
}
