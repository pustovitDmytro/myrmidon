{
  "name": "myrmidon",
  "version": "1.10.0",
  "description": "package myrmidon provides a list of common utilities for comfortable work with js projects.",
  "main": "./lib",
  "scripts": {
    "prepare": "npm run build:src && husky install",
    "build:src": "babel src --out-dir ./lib --copy-files",
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "test:security": "npm audit --production && npm run test:lock",
    "test:lint": "eslint src bin tests --no-error-on-unmatched-pattern",
    "test:mocha": "mocha --config .mocharc.json 'tests/**/*test.js'",
    "test:package": "npt test -c .package-tester.json",
    "test:coverage": "BUILD=0 MODE=test nyc mocha --config .mocharc.json 'tests/**/*test.js'",
    "test:debt": "jscpd src --config .jscpd.json",
    "test:lock": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm",
    "test:build": "npm run build && BUILD=1 mocha --config .mocharc.json 'tests/**/*.test.js'",
    "save-examples": "SAVE_EXAMPLES=tmp/examples.json mocha  --config .mocharc.json 'tests/**/*.test.js' --grep '@example'",
    "coverage-check": "MODE=test nyc check-coverage",
    "test": "npm run test:lint && npm run test:security && BUILD=1 npm run test:mocha",
    "sort-deps": "npm remove --save example; npm remove --save-dev example",
    "semantic-release": "npx semantic-release",
    "documentate:reference": "code-chronicle --config=./.code-chronicle.json templates/documentation/docs docs && code-chronicle --config=./.code-chronicle.json .github/CONTRIBUTING.md docs/CONTRIBUTING.md",
    "documentate:readme": "code-chronicle --config=./.code-chronicle.json templates/documentation/readme.md README.md",
    "documentate": "npm run documentate:reference && npm run documentate:readme"
  },
  "keywords": [
    "utilities",
    "modules",
    "utils",
    "js",
    "helpers"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pustovitDmytro/myrmidon"
  },
  "author": "Dmytro Pustovit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pustovitDmytro/myrmidon/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://myrmidonjs.readthedocs.io",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.23.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/lint": "^19.5.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "chai": "^5.1.1",
    "chance": "^1.1.11",
    "cls-hooked": "^4.2.2",
    "code-chronicle": "^1.5.2",
    "conventional-changelog-eslint": "^6.0.0",
    "coveralls": "^3.1.1",
    "danger": "^12.3.3",
    "eslint": "^9.13.0",
    "eslint-config-incredible": "^2.4.4",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-censor": "^1.5.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-no-secrets": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-unicorn": "56.0.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.6",
    "jscpd": "^4.0.5",
    "lockfile-lint": "^4.12.1",
    "mocha": "^10.7.3",
    "mocha-junit-reporter": "^2.2.1",
    "node-package-tester": "^1.3.6",
    "nyc": "^17.1.0",
    "reflect-metadata": "^0.2.1",
    "semantic-release": "^24.1.3",
    "semantic-release-telegram": "^1.6.2",
    "uuid": "^10.0.0"
  }
}
